coord = function handledata(past)

data = past(:,2:3,:);

coord = [];
for i = size(data,3)
    new(1:2,:) = [data(2*i-1,:) ; data(2*i,:)];    
    coord = [coord new];
end

coord = coord';
% Have to switch columns for (x,y) mix-up
coordtemp = coord;
coord(:,1) = coordtemp(:,2);
coord(:,2) = coordtemp(:,1);

end